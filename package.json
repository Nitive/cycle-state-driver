{
  "name": "cycle-state-driver",
  "version": "0.1.0",
  "description": "Redux like state driver for Cycle.js",
  "scripts": {
    "clean": "rm -rf extra/ lib/ index.*",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "lib/",
    "extra/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nitive/cycle-state-driver.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nitive/cycle-state-driver/issues"
  },
  "homepage": "https://github.com/Nitive/cycle-state-driver#readme",
  "devDependencies": {
    "@types/jest": "^16.0.1",
    "codeclimate-test-reporter": "^0.4.0",
    "jest": "^18.0.0",
    "ts-jest": "^18.0.0",
    "tslint": "^4.0.2",
    "tslint-config-standard": "^2.0.0",
    "typescript": "^2.1.0",
    "xstream": "^9.0.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "xstream": ">= 9.0.0"
  },
  "jest": {
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": ".*test\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
